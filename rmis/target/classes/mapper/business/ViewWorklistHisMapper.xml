    <select id="queryViewWorklist" parameterType="com.jfhealthcare.modules.business.request.ViewWorklistRequest"
            resultMap="ResponseMap">
        select<include refid="Base_Column_List"/>,imageNum
        from (
        select y.*, xx.CHECK_REP_GROUP_ID, xx.status GROUP_PROPERTIES
        from (
        select<include refid="View_Column_List"/>,
        (select name from sys_operator where logincode=a.CHECK_REPORT_DR) CHECK_REPORT_DR_NAME,
        (select signature from sys_operator where logincode=a.CHECK_REPORT_DR) CHECK_REPORT_DR_SIGNA,
        (select name from sys_operator where logincode=a.CHECK_AUDIT_DR) CHECK_AUDIT_DR_NAME,
        (select signature from sys_operator where logincode=a.CHECK_AUDIT_DR) CHECK_AUDIT_DR_SIGNA,
        (select signature from sys_operator where logincode=a.CHECK_PRINT_DR) CHECK_PRINT_DR_SIGNA,
        a.CHECK_IMG_NUM imageNum
        from view_worklist_his a
        <where>
            <if test="checkJzFlag !=null">
                and a.CHECK_JZ_FLAG=#{checkJzFlag,jdbcType=BIT}
            </if>
            <if test="checkVjFlag !=null">
                and a.CHECK_VJ_FLAG=#{checkVjFlag,jdbcType=BIT}
            </if>
            <if test="checkAccessionNum !=null and checkAccessionNum !=''">
                and a.CHECK_ACCESSION_NUM=#{checkAccessionNum,jdbcType=VARCHAR}
            </if>
            <if test="checkNum !=null and checkNum !=''">
                and a.CHECK_NUM=#{checkNum,jdbcType=VARCHAR}
            </if>
            <if test="checkStatusCodes ==null or checkStatusCodes.size &lt;= 0">
                and a.CHECK_STATUS_CODE != '3560'
            </if>
            <if test="checkStatusCodes !=null and checkStatusCodes.size>0">
                and a.CHECK_STATUS_CODE in
                <foreach item="item" collection="checkStatusCodes" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="checkStatusAiCode !=null and checkStatusAiCode !=''">
                and a.CHECK_STATUS_AI_CODE=#{checkStatusAiCode,jdbcType=VARCHAR}
            </if>
            <if test="checkTypeCodes !=null and checkTypeCodes.size>0">
                and a.CHECK_TYPE_CODE in
                <foreach item="item" collection="checkTypeCodes" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="checkSummaryCode !=null and checkSummaryCode !=''">
                and a.CHECK_SUMMARY_CODE=#{checkSummaryCode,jdbcType=VARCHAR}
            </if>
            <if test="patName !=null and patName !=''">
                and a.PAT_NAME like concat(concat('%',#{patName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="patSexCode !=null and patSexCode !=''">
                and a.PAT_SEX_CODE=#{patSexCode,jdbcType=VARCHAR}
            </if>
            <if test="checkApplyStartTime !=null and checkApplyEndTime !=null">
                and a.CHECK_APPLY_TIME &gt;= #{checkApplyStartTime,jdbcType=TIMESTAMP}
                and a.CHECK_APPLY_TIME &lt;= #{checkApplyEndTime,jdbcType=TIMESTAMP}
            </if>
            <if test="checkRepStartTime !=null and checkRepEndTime !=null">
                and a.CHECK_REPORT_TIME &gt;= #{checkRepStartTime,jdbcType=TIMESTAMP}
                and a.CHECK_REPORT_TIME &lt;= #{checkRepEndTime,jdbcType=TIMESTAMP}
            </if>
            <if test="checkApplyDoc !=null and checkApplyDoc !=''">
                and a.CHECK_APPLY_DOC like concat(concat('%',#{checkApplyDoc,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkReportDr !=null and checkReportDr !=''">
                and a.CHECK_REPORT_DR like concat(concat('%',#{checkReportDr,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkAuditDr !=null and checkAuditDr !=''">
                and a.CHECK_AUDIT_DR like concat(concat('%',#{checkAuditDr,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkApplyHospCode !=null and checkApplyHospCode !=''">
                and a.CHECK_APPLY_HOSP_CODE=#{checkApplyHospCode,jdbcType=VARCHAR}
            </if>
            <if test="checkParts !=null and checkParts !=''">
                and a.CHECK_PARTS like concat(concat('%',#{checkParts,jdbcType=VARCHAR}),'%')
            </if>
            <if test="reprcdImpression1 !=null and reprcdImpression1 !=''">
                and a.REPRCD_IMPRESSION1 like concat(concat('%',#{reprcdImpression1,jdbcType=VARCHAR}),'%')
            </if>
            <if test="reprcdHp !=null and reprcdHp !=''">
                and a.REPRCD_HP=#{reprcdHp,jdbcType=VARCHAR}
            </if>
            <if test="ptnSource !=null and ptnSource !=''">
                and a.CHECK_PTN_SOURCE=#{ptnSource,jdbcType=VARCHAR}
            </if>
        </where>
        ) y inner join
        (select max(a.group_id) CHECK_REP_GROUP_ID, b.hospital_id hospital_id, d.status status
        from sys_rep_group a, sys_org_group_rel b, sys_oper_org_rel c,rep_group d
        <where>
            a.group_id = c.oper_group_id
            and b.group_id = c.org_group_id
            and a.group_id = d.id
            and b.is_valid = 1
            and c.is_valid = 1
            <if test="loginCode !=null and loginCode !=''">
                and a.logincode =#{loginCode,jdbcType=VARCHAR}
            </if>
            <if test="checkRepGroupId !=null and checkRepGroupId !=''">
                and a.group_id=#{checkRepGroupId,jdbcType=VARCHAR}
            </if>
        </where>
        group by b.hospital_id, d.status
        ) xx on
        y.CHECK_APPLY_HOSP_CODE = xx.hospital_id
        and ((y.DOC_LEVEL = 0 and xx.status = '0') or (y.DOC_LEVEL = 1 and xx.status = '1'))
        <where>
            <if test="checkReportDrName !=null and checkReportDrName !=''">
                and y.CHECK_REPORT_DR_NAME like concat(concat('%',#{checkReportDrName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkAuditDrName !=null and checkAuditDrName !=''">
                and y.CHECK_AUDIT_DR_NAME like concat(concat('%',#{checkAuditDrName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkApplyDocCode !=null and checkApplyDocCode !=''">
                and y.check_apply_doc_code like concat(concat('%',#{checkApplyDocCode,jdbcType=VARCHAR}),'%')
            </if>
        </where>
        ) z ORDER BY CHECK_JZ_FLAG desc, CHECK_APPLY_TIME desc
    </select>

    <select id="queryViewWorklist_COUNT" parameterType="com.jfhealthcare.modules.business.request.ViewWorklistRequest"
            resultType="Long">
        select count(1) from (
        select 1
        <if test="checkReportDrName !=null and checkReportDrName !=''">
            ,(select name from sys_operator where logincode=a.CHECK_REPORT_DR) CHECK_REPORT_DR_NAME
        </if>
        <if test="checkAuditDrName !=null and checkAuditDrName !=''">
            ,(select name from sys_operator where logincode=a.CHECK_AUDIT_DR) CHECK_AUDIT_DR_NAME
        </if>
        from view_worklist_his a
        <where>
            <if test="checkJzFlag !=null">
                and a.CHECK_JZ_FLAG=#{checkJzFlag,jdbcType=BIT}
            </if>
            <if test="checkVjFlag !=null">
                and a.CHECK_VJ_FLAG=#{checkVjFlag,jdbcType=BIT}
            </if>
            <if test="checkAccessionNum !=null and checkAccessionNum !=''">
                and a.CHECK_ACCESSION_NUM=#{checkAccessionNum,jdbcType=VARCHAR}
            </if>
            <if test="checkNum !=null and checkNum !=''">
                and a.CHECK_NUM=#{checkNum,jdbcType=VARCHAR}
            </if>
            <if test="checkStatusCodes ==null or checkStatusCodes.size &lt;= 0">
                and a.CHECK_STATUS_CODE != '3560'
            </if>
            <if test="checkStatusCodes !=null and checkStatusCodes.size>0">
                and a.CHECK_STATUS_CODE in
                <foreach item="item" collection="checkStatusCodes" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="checkStatusAiCode !=null and checkStatusAiCode !=''">
                and a.CHECK_STATUS_AI_CODE=#{checkStatusAiCode,jdbcType=VARCHAR}
            </if>
            <if test="checkTypeCodes !=null and checkTypeCodes.size>0">
                and a.CHECK_TYPE_CODE in
                <foreach item="item" collection="checkTypeCodes" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="checkSummaryCode !=null and checkSummaryCode !=''">
                and a.CHECK_SUMMARY_CODE=#{checkSummaryCode,jdbcType=VARCHAR}
            </if>
            <if test="patName !=null and patName !=''">
                and a.PAT_NAME like concat(concat('%',#{patName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="patSexCode !=null and patSexCode !=''">
                and a.PAT_SEX_CODE=#{patSexCode,jdbcType=VARCHAR}
            </if>
            <if test="checkApplyStartTime !=null and checkApplyEndTime !=null">
                and a.CHECK_APPLY_TIME &gt;= #{checkApplyStartTime,jdbcType=TIMESTAMP}
                and a.CHECK_APPLY_TIME &lt;= #{checkApplyEndTime,jdbcType=TIMESTAMP}
            </if>
            <if test="checkRepStartTime !=null and checkRepEndTime !=null">
                and a.CHECK_REPORT_TIME &gt;= #{checkRepStartTime,jdbcType=TIMESTAMP}
                and a.CHECK_REPORT_TIME &lt;= #{checkRepEndTime,jdbcType=TIMESTAMP}
            </if>
            <if test="checkApplyDoc !=null and checkApplyDoc !=''">
                and a.CHECK_APPLY_DOC like concat(concat('%',#{checkApplyDoc,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkReportDr !=null and checkReportDr !=''">
                and a.CHECK_REPORT_DR like concat(concat('%',#{checkReportDr,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkAuditDr !=null and checkAuditDr !=''">
                and a.CHECK_AUDIT_DR like concat(concat('%',#{checkAuditDr,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkApplyHospCode !=null and checkApplyHospCode !=''">
                and a.CHECK_APPLY_HOSP_CODE=#{checkApplyHospCode,jdbcType=VARCHAR}
            </if>
            <if test="checkParts !=null and checkParts !=''">
                and a.CHECK_PARTS like concat(concat('%',#{checkParts,jdbcType=VARCHAR}),'%')
            </if>
            <if test="reprcdImpression1 !=null and reprcdImpression1 !=''">
                and a.REPRCD_IMPRESSION1 like concat(concat('%',#{reprcdImpression1,jdbcType=VARCHAR}),'%')
            </if>
            <if test="reprcdHp !=null and reprcdHp !=''">
                and a.REPRCD_HP=#{reprcdHp,jdbcType=VARCHAR}
            </if>
            <if test="ptnSource !=null and ptnSource !=''">
                and a.CHECK_PTN_SOURCE=#{ptnSource,jdbcType=VARCHAR}
            </if>
            and exists(select 1 from sys_rep_group aa, sys_org_group_rel bb, sys_oper_org_rel cc,rep_group dd
            where aa.group_id = cc.oper_group_id
            and bb.group_id = cc.org_group_id
            and aa.group_id = dd.id
            and bb.is_valid = 1
            and cc.is_valid = 1
            <if test="loginCode !=null and loginCode !=''">
                and aa.logincode =#{loginCode,jdbcType=VARCHAR}
            </if>
            <if test="checkRepGroupId !=null and checkRepGroupId !=''">
                and aa.group_id=#{checkRepGroupId,jdbcType=VARCHAR}
            </if>
            and a.CHECK_APPLY_HOSP_CODE = bb.hospital_id
            and ((a.DOC_LEVEL = 0 and dd.status = '0') or (a.DOC_LEVEL = 1 and dd.status = '1'))
            )
        </where>
        ) y
        <where>
            <if test="checkReportDrName !=null and checkReportDrName !=''">
                and y.CHECK_REPORT_DR_NAME like concat(concat('%',#{checkReportDrName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkAuditDrName !=null and checkAuditDrName !=''">
                and y.CHECK_AUDIT_DR_NAME like concat(concat('%',#{checkAuditDrName,jdbcType=VARCHAR}),'%')
            </if>
            <if test="checkApplyDocCode !=null and checkApplyDocCode !=''">
                and y.check_apply_doc_code like concat(concat('%',#{checkApplyDocCode,jdbcType=VARCHAR}),'%')
            </if>
        </where>
    </select>

    <select id="queryOneViewWorklist" parameterType="com.jfhealthcare.modules.business.request.ViewWorklistRequest"
            resultMap="ResponseMap">
        select
        <include refid="Base_Column_List"/>
        from ( select a.*, '0' GROUP_PROPERTIES,
        (select name from sys_operator where logincode=a.CHECK_REPORT_DR) CHECK_REPORT_DR_NAME,
        (select signature from sys_operator where logincode=a.CHECK_REPORT_DR) CHECK_REPORT_DR_SIGNA,
        (select name from sys_operator where logincode=a.CHECK_AUDIT_DR) CHECK_AUDIT_DR_NAME,
        (select signature from sys_operator where logincode=a.CHECK_AUDIT_DR) CHECK_AUDIT_DR_SIGNA,
        (select signature from sys_operator where logincode=a.CHECK_PRINT_DR) CHECK_PRINT_DR_SIGNA
        from view_worklist_his a
        <where>
            <if test="checkAccessionNum !=null and checkAccessionNum !=''">
                and a.CHECK_ACCESSION_NUM=#{checkAccessionNum,jdbcType=VARCHAR}
            </if>
        </where>
        ) b
    </select>

    <select id="queryCountViewWorklist" parameterType="com.jfhealthcare.modules.business.request.ViewWorklistRequest"
            resultType="Map">
        select a.OTHERVALUE name,a.id code,IFNULL(b.num,0) num
        from sys_dict_dtl a
        left join (
        select count(0) num,CHECK_STATUS_CODE from view_worklist_his a
        <where>
            <if test="checkTypeCodes !=null and checkTypeCodes.size>0">
                and a.CHECK_TYPE_CODE in
                <foreach item="item" collection="checkTypeCodes" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="checkApplyStartTime !=null and checkApplyEndTime !=null">
                and a.CHECK_APPLY_TIME &gt;= #{checkApplyStartTime,jdbcType=TIMESTAMP}
                and a.CHECK_APPLY_TIME &lt;= #{checkApplyEndTime,jdbcType=TIMESTAMP}
            </if>
            <if test="checkApplyHospCode !=null and checkApplyHospCode !=''">
                and a.CHECK_APPLY_HOSP_CODE=#{checkApplyHospCode,jdbcType=VARCHAR}
            </if>
            and exists(select 1 from sys_rep_group aa, sys_org_group_rel bb, sys_oper_org_rel cc,rep_group dd
            where aa.group_id = cc.oper_group_id
            and bb.group_id = cc.org_group_id
            and aa.group_id = dd.id
            and bb.is_valid = 1
            and cc.is_valid = 1
            <if test="loginCode !=null and loginCode !=''">
                and aa.logincode =#{loginCode,jdbcType=VARCHAR}
            </if>
            <if test="checkRepGroupId !=null and checkRepGroupId !=''">
                and aa.group_id=#{checkRepGroupId,jdbcType=VARCHAR}
            </if>
            and a.CHECK_APPLY_HOSP_CODE = bb.hospital_id
            and ((a.DOC_LEVEL = 0 and dd.status = '0') or (a.DOC_LEVEL = 1 and dd.status = '1'))
            )
        </where>
        group by CHECK_STATUS_CODE
        ) b on a.id=b.CHECK_STATUS_CODE
        <where>
            a.code='checkStatus'
        </where>
        order by a.id ASC
    </select>

</mapper>